hostname = nodeloc.com, www.nodeloc.com, nodeseek.com, www.nodeseek.com, *.barventory.com, api.example.com

# ============ 数据查看器 ============
# 访问: http://api.example.com/qxdata

^https?://api\.example\.com/qxdata/delete url script-echo-response https://raw.githubusercontent.com/umalaaa/qccfig-mirror/main/scripts/data_delete.js
^https?://api\.example\.com/qxdata/clear url script-echo-response https://raw.githubusercontent.com/umalaaa/qccfig-mirror/main/scripts/data_clear.js
^https?://api\.example\.com/qxdata url script-echo-response https://raw.githubusercontent.com/umalaaa/qccfig-mirror/main/scripts/data_viewer.js

# ============ Cookie 捕获 ============
^https?://(www\.)?nodeloc\.com/.* url script-request-header https://raw.githubusercontent.com/umalaaa/qccfig-mirror/main/scripts/cookie_capture.js
^https?://(www\.)?nodeseek\.com/.* url script-request-header https://raw.githubusercontent.com/umalaaa/qccfig-mirror/main/scripts/cookie_capture.js

# ============ Barventory Response 记录 ============
^https?://.*\.barventory\.com/.* url script-response-body https://raw.githubusercontent.com/umalaaa/qccfig-mirror/main/scripts/response_logger.js
